<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Telemedicine</title>
</head>

<body>
    <table>
        <thead>
            <tr>
                <th> Patient ID </th>
                <th> First Name </th>
                <th> Last Name </th>
                <th>Date of Birth</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(row=>{%>
                <tr>
                    <td>
                        <%= row.patient_id %>
                    </td>

                    <td>
                        <%= row.first_name %>
                    </td>
                    <td>
                        <%= row.last_name %>
                    </td>
                    <td>
                        <%= row.date_of_birth %>
                    </td>
                </tr>
                <%});%>
        </tbody>
    </table>
</body>

</html>